package javabasics;

public class PrintPrintlnDemo {

	public static void main(String[] args) {
		// ln means line
		// System.out.println() - after printing the cursor moves to the new line
		// System.out.print() - after printing the cursor doen not move to the new line
		
		System.out.print("Hello, How are you?");
		System.out.print("I am good. Thankyou. How about you?");
		System.out.print("I am fine.");
		
		System.out.println();
		System.out.println("----------------------");
		System.out.println("Hello, How are you?");
		System.out.println("I am good. Thankyou. How about you?");
		System.out.println("I am fine.");

	}

}
