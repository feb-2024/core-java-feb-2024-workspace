package classesandobjects;

public class AnimalDemo {

	public static void main(String[] args) {
		Animal animal1 = new Animal();
		animal1.breathe();
		animal1.mobility();
		
		System.out.println("---------------");
		
		Dog dog1 = new Dog();
		dog1.breathe();
		dog1.mobility();
		dog1.friendly();
		
		System.out.println("---------------");
		
		Snake snake1 = new Snake();
		snake1.breathe();
		snake1.mobility();
		snake1.attacks();

	}

}
